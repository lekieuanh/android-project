<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <stroke android:color="@color/colorBorderBtn"
                android:width="1dp"/>
            <corners android:radius="5dp"/>
            <solid android:color="#92b843"/>
